{
  "name": "tauri-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode development",
    "build": "run-p type-check build-only-prod",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only-prod": "vite build --mode prod",
    "build-only-prod:netlify": "vite build --mode netlify",
    "build-only-sit": "vite build --mode sit",
    "build-only-uat": "vite build --mode uat",
    "build-only-development": "vite build --mode development",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "npx prettier . --write",
    "format:check": "npx prettier . --check",
    "docs": "mdbook serve docs",
    "tauri": "tauri dev",
    "tauri:build": "tauri build",
    "test:e2e": "npx cypress open",
    "socket-server": "npm run -C server/socket-chat dev"
  },
